<table cellspacing="2" cellpadding="2" border="0">
  <tr>
   @model.mainMenu.forEach(function (item) {
      <td><a href="@item.path" style="text-decoration: none"><font size="-1" face="arial" color="#FFFFFF">@item.label</font></a></td>
      <td><font size="-1" face="arial" color="#FFFFFF">|</font></td>
   })
  </tr>
</table>