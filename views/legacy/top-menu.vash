<table border="0" cellspacing="5">
  <tr>
    @model.mainMenu.forEach(function (item) {
      <td align="center">
        <a href="@item.path">
          <img src="@item.icon">
          <br>
          <font size="-1">@item.label</font>
        </a>
      </td>
    })                
  </tr>
</table>