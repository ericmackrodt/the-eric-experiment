@{
  var categories = Object.keys(model.categories);
}
<div id="side-content">
<h3>Categories</h3>
<table cellspacing="0" cellpadding="0">
  @categories.forEach(function (item) {
    <tr>
      <td>
        <a href="/category?id=@item">
          @item
        </a>
      </td>
    </tr>
  })
</table>

<hr />

<center>
  <img src="/assets/200.gif" width="88" height="31" />
  <br />
  <img src="/assets/netscap4.gif" />
</center>
</div>